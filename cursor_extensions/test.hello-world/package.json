{
	"name": "hello-world",
	"displayName": "Hello World",
	"description": "A minimal extension to test Cursor extension loading",
	"version": "0.0.1",
	"publisher": "test",
	"engines": {
		"vscode": "^1.73.0",
		"cursor": "^1.0.0"
	},
	"activationEvents": [
		"onCommand:hello-world.sayHello",
		"onCommand:hello-world.showMarkdownWebview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hello-world.sayHello",
				"title": "Hello World: Say Hello"
			},
			{
				"command": "hello-world.showMarkdownWebview",
				"title": "Hello World: Show Markdown Webview"
			}
		]
	},
	"__metadata": {
		"size": 826
	},
	"repository": {
		"type": "git",
		"url": "https://example.com/repo.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"dependencies": {
		"typescript": "^5.8.3"
	},
	"devDependencies": {
		"@types/node": "^22.15.30"
	}
}
