{
   "applications": {
      "gecko": {
         "id": "languagetool-webextension@languagetool.org"
      }
   },
   "author": "Daniel Naber",
   "background": {
      "scripts": [ "bs/background.js", "share/tools.js", "bs/common.js", "share/markup.js" ]
   },
   "browser_action": {
      "browser_style": false,
      "default_icon": "images/icon48.png",
      "default_popup": "popup.html",
      "default_title": "__MSG_tooltip__"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+Space"
         }
      }
   },
   "content_scripts": [ {
      "css": [ "vendors/featherlight/featherlight.css", "css/lt-marker-style.css" ],
      "js": [ "vendors/jquery.min.js", "vendors/purify.js", "vendors/ally.js", "vendors/most.js", "vendors/featherlight/featherlight.js", "share/helpers.js", "share/tools.js", "share/markup.js", "cs/contentscript.js", "cs/textarea.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*" ]
   }, {
      "js": [ "cs/contentscript-lt.js" ],
      "matches": [ "https://languagetool.org/*", "https://www.languagetool.org/*" ]
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "homepage_url": "https://languagetool.org",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "64": "images/icon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "LanguageTool - Grammar and Style Checker",
   "offline_enabled": false,
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "activeTab", "storage", "contextMenus" ],
   "short_name": "LanguageTool",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.38",
   "web_accessible_resources": [ "images/*", "**/*.js", "vendors/*", "popup.html" ]
}
