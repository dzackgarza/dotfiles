{
   "background": {
      "scripts": [ "libs/primus.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "title": "PomoDone it!"
   },
   "content_scripts": [ {
      "js": [ "inject.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   }, {
      "css": [ "domain/todoist.com.css" ],
      "js": [ "libs/riot.compiler.min.js", "libs/zepto.min.js", "domain/todoist.com.js", "pd-button.tag.js" ],
      "matches": [ "*://*.todoist.com/*" ]
   }, {
      "js": [ "libs/riot.compiler.min.js", "libs/zepto.min.js", "domain/docs.google.com.js", "pd-button.tag.js" ],
      "matches": [ "*://docs.google.com/*" ]
   }, {
      "css": [ "domain/trello.css" ],
      "js": [ "libs/riot.compiler.min.js", "libs/zepto.min.js", "domain/trello.js", "pd-button.tag.js", "pd-info.tag.js" ],
      "matches": [ "*://trello.com/*" ]
   }, {
      "css": [ "domain/my.pomodoneapp.css" ],
      "js": [ "libs/riot.compiler.min.js", "libs/zepto.min.js", "domain/my.pomodoneapp.js", "pd-button.tag.js" ],
      "matches": [ "*://my.pomodoneapp.com/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "PomoDoneApp Extension",
   "icons": {
      "128": "icon128.png",
      "16": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "PomoDoneApp Extension",
   "options_page": "options.html",
   "permissions": [ "http://*/*", "https://*/*", "tabs", "contextMenus", "management", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.7",
   "web_accessible_resources": [ "icon128.png", "stop.html" ]
}
